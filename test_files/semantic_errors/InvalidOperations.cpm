class MyClass {
  volatile i1: int
  i2: int
  volatile b1: boolean
  b2: boolean
  volatile ia1: int[]
  ia2: int[]
  c1: ClassDatatest
  c2: ClassDatatest

  semantically_incorrect_expressions(): boolean {

    i1 := i1 + b1 //@error - semantic (invalid operand type: int + boolean)
    i1 := i1 + ia1 //@error - semantic (invalid operand type: int + int[])
    i1 := i1 + c1 //@error - semantic (invalid operand type: int + ClassDatatest)

    i1 := b1 + b1 //@error - semantic (invalid operand type: boolean + boolean)
    i1 := ia1 + ia1 //@error - semantic (invalid operand type: int[] + int[])
    i1 := ia1 + ia1[0] //@error - semantic (invalid operand type: int[] + int)
