class A {

  a1(num: int): A {
    num_aux: int[]

    num_aux[false] := 2 //@error - semantic (invalid type of array index)

    if (num_aux[a2()] < 1) { //@error - semantic (invalid type of array index)
      num_aux[0] := 1
    } else {
      num_aux := a2() //@error - semantic (type mismatch: int[] := int)
    }

    num_aux[3] := num.length //@error - semantic (member .length is used incorrectly)

    return this //@error - semantic (invalid use of 'this' in C+-)
  }

