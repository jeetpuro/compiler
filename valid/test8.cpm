class Calculator {

  sumArray(values : int[]): int {
    volatile i : int := 0
    volatile total : int := 0

    for (i := 0, i < values.length, i := i + 1) {
      total := total + values[i]
    }

    return total
  }
}

class Counter {
  volatile value : int

  init(v : int): void {
    value := v
  }

  inc(): void {
    value := value + 1
  }

  get(): int {
    return value
  }
}

main(): int {
    volatile data : int[] := int[3, 1, 4, 1, 5]
    volatile sum : int := Calculator().sumArray(data)

    print(sum)

    volatile c : Counter := Counter()
    c.init(0)

    volatile i : int := 0
    for (i := 0, i < data.length, i := i + 1) {
      c.inc()
    }

    print(c.get())
  return 0
}